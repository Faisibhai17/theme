{
  "content": "{% if section.settings.footer_style == 'footer_style_1' %}\n  <footer class=\"footer-area\" id=\"section-{{ section.id }}\">\n    <div class=\"{% if section.settings.container_style == 'style_1' %}container {% else %} container-fluid{% endif %}\">\n      {% include 'footer-top' %}\n    </div>\n  </footer>\n\n{% elsif section.settings.footer_style == 'footer_style_2' %}\n  {% if template == 'index' %}\n    <footer\n      class=\"footer-area 1 {% if section.settings.container_style == 'style_1' %} {% else %} hm4-footer-padding{% endif %}\"\n      id=\"section-{{ section.id }}\"\n    >\n      <div class=\"{% if section.settings.container_style == 'style_1' %}container {% else %} container-fluid{% endif %}\">\n        {% include 'footer-top' %}\n        <div class=\"row f_btm\">\n          <div class=\"col-lg-6 col-md-6 col-12 payment-icons-first\">\n            <div class=\"pay_icn\">{% include 'footer-payment-area' %}</div>\n            <span>© <span id=\"current-year\"></span> Yessé Australia. All Rights Reserved.</span>\n          </div>\n          <div class=\"col-lg-6 col-md-6 col-12 social-icons-first\">{% include 'footer-social-area' %}</div>\n        </div>\n      </div>\n    </footer>\n  {% else %}\n    <footer class=\"footer-area 2\" id=\"section-{{ section.id }}\">\n      <div class=\"container \">\n        {% include 'footer-top' %}\n        <div class=\"row f_btm\">\n          <div class=\"col-lg-6 col-md-6 col-12 payment-icons-second\">\n            <div class=\"pay_icn\">{% include 'footer-payment-area' %}</div>\n            <span>© <span id=\"current-year\"></span> Yessé Australia. All Rights Reserved.</span>\n          </div>\n          <div class=\"col-lg-6 col-md-6 col-12 social-icons-second\">{% include 'footer-social-area' %}</div>\n        </div>\n      </div>\n    </footer>\n  {% endif %}\n\n{% elsif section.settings.footer_style == 'footer_style_3' %}\n  <footer class=\"footer-area\" id=\"section-{{ section.id }}\">\n    {% include 'footer-top-2' %}\n    {% include 'footer-bottom' %}\n    {% section 'collection-banner' %}\n  </footer>\n\n{% else %}\n  <footer class=\"footer-area footer-white\" id=\"section-{{ section.id }}\">\n    {% include 'footer-top-2' %}\n  </footer>\n  <style>\n    .icon {\n      width: 3.125em;\n    }\n    .visuallyhidden {\n      border: 0;\n      clip: rect(0 0 0 0);\n      height: 1px;\n      margin: -1px;\n      overflow: hidden;\n      padding: 0;\n      position: absolute;\n      width: 1px;\n      white-space: nowrap;\n    }\n  </style>\n\n  {%- unless shop.enabled_payment_types == empty -%}\n    <span class=\"visuallyhidden\">Supported payment methods</span>\n\n    <ul>\n      <ul class=\"list\" role=\"list\">\n        <li>\n          <img\n            alt=\"Google pay\"\n            class=\"footer-icon\"\n            src=\"https://cdn.shopify.com/s/files/1/0871/1864/3519/files/google_pay.png?v=1714714798\"\n            loading=\"lazy\"\n            style=\"    background: red;\"\n          >\n        </li>\n        <li>\n          <img\n            alt=\"Google pay\"\n            class=\"footer-icon\"\n            src=\"https://cdn.shopify.com/s/files/1/0555/6148/6492/files/1_29f07d6c-68cf-4e7b-b6fc-746d6567f295.png?v=1723206830\"\n            loading=\"lazy\"\n          >\n        </li>\n        <li>\n          <img\n            alt=\"Shop Pay\"\n            class=\"footer-icon\"\n            src=\"https://cdn.shopify.com/s/files/1/0555/6148/6492/files/ShopPay.png?v=1723529397\"\n            loading=\"lazy\"\n          >\n        </li>\n        <li>\n          <img\n            alt=\"Google pay\"\n            class=\"footer-icon\"\n            src=\"https://cdn.shopify.com/s/files/1/0871/1864/3519/files/imgpsh_fullsize_anim_7_6d518ea3-fbf6-4c23-b199-474b5314a8eb.png?v=1714746791\"\n            loading=\"lazy\"\n          >\n        </li>\n        <li>\n          <img\n            alt=\"Google pay\"\n            class=\"footer-icon\"\n            src=\"https://cdn.shopify.com/s/files/1/0871/1864/3519/files/imgpsh_fullsize_anim_9_9af8fa7a-22c8-4cb0-ad50-34faaef98f5f.png?v=1714746792\"\n            loading=\"lazy\"\n          >\n        </li>\n        <li>\n          <img\n            alt=\"Google pay\"\n            class=\"footer-icon\"\n            src=\"https://cdn.shopify.com/s/files/1/0871/1864/3519/files/imgpsh_fullsize_anim_10_40267753-762c-4576-aaf3-684e3c0931df.png?v=1714746791\"\n            loading=\"lazy\"\n          >\n        </li>\n        <li>\n          <img\n            alt=\"Google pay\"\n            class=\"footer-icon\"\n            src=\"https://cdn.shopify.com/s/files/1/0871/1864/3519/files/imgpsh_fullsize_anim_8_e608be74-5cec-4235-98b9-0dcb10b58155.png?v=1714746792\"\n            loading=\"lazy\"\n          >\n        </li>\n      </ul>\n    </ul>\n  {%- endunless -%}\n{% endif %}\n\n{%- include 'section-style' -%}\n\n<div class=\"footer-block__details-content\">\n  {{ block.settings.html_content }}\n</div>",
