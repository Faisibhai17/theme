  if ( $('.osf_close_button').length > 0 ) {
    if ( !$('.osf_close_button').hasClass('notranslate') ) {
      $('.osf_close_button').addClass('notranslate')  
    }
  }

  if ( !hasSet && $('.div.osf_dialog > div > div > div > button > span').length > 0 ) {
    hasSet = true;
    $('.div.osf_dialog > div > div > div > button > span').addClass('hide-close-btn')
  }
},100)


setTimeout(function() {
  let body = document.querySelector('body')
  body.addEventListener('click', function(e) {
      let target = e.target
      if ( target.className === 'osf_input osf_input--error' || target.className == 'osf_input' ) {
        let html = $('body').find('.osf_close_span').html()
        let text = $('div.osf_dialog > div > div > div > button > span').html()
        let checkClose = text.toString()
        let numberOfClose = (checkClose.match(/×/g) || 0).length;

        if ( numberOfClose > 1 ) {
          let checkText = text.toString().substr(text.toString().length - 1, 1)
          if ( checkText == "×" ) {
            text = text.substr(0, text.length - 1)
            $('body').find('.osf_close_span').html(text)
            $('.div.osf_dialog > div > div > div > button > span').removeClass('hide-close-btn')
          }
        }
             
      }
  })
},100)
</script>

