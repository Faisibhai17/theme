{
  "content": "{% if section.settings.team_area_enable %}\n  <div class=\"team-area\" id=\"section-{{ section.id }}\">\n    <div class=\"{% if section.settings.container_style == 'style_1' %}container {% else %} container-fluid{% endif %}\">\n      <div class=\"section-title-2 text-{% if section.settings.section_text_style == 'section_style_left' %}left{% elsif section.settings.section_text_style == 'section_style_center' %}center{% elsif section.settings.section_text_style == 'section_style_right' %}right{% endif %} mb-60\">\n        {% if section.settings.section_title != '' %}\n          <h2>{{ section.settings.section_title }}</h2>\n        {% endif %}\n        {% if section.settings.section_description != '' %}\n          <p>{{ section.settings.section_description }}</p>\n        {% endif %}\n      </div>\n      <div class=\"row\">\n        {% for block in section.blocks %}\n          {% case block.type %}\n            {% when 'team' %}\n              <div class=\"col-lg-{{ block.settings.team_coloum }} col-md-6 col-12\">\n                <div class=\"team-wrapper mb-30\">\n                  <div class=\"team-img\">\n                    <a href=\"{{ block.settings.team_image_url }}\">\n                      <img\n                        src=\"{% if block.settings.team_image %}{{ block.settings.team_image | img_url: 'large' }}{% else %}http:\\/\\/via.placeholder.com\\/390x450{% endif %}\"\n                        alt=\"{{ team_image.alt }}\"\n                      >\n                    </a>\n\n                    <div class=\"team-action\">\n                      {% if block.settings.social_icon_1_name != '' %}\n                        <a class=\"facebook\" href=\"{{ block.settings.socila_icon_1_link }}\">\n                          <i class=\"fa fa-{{ block.settings.social_icon_1_name }}\"></i>\n                        </a>\n                      {% endif %}\n                      {% if block.settings.social_icon_2_name != '' %}\n                        <a class=\"twitter\" title=\"Wishlist\" href=\"{{ block.settings.socila_icon_2_link }}\">\n                          <i class=\"fa fa-{{ block.settings.social_icon_2_name }}\"></i>\n                        </a>\n                      {% endif %}\n                      {% if block.settings.social_icon_3_name != '' %}\n                        <a class=\"instagram\" href=\"{{ block.settings.socila_icon_3_link }}\">\n                          <i class=\"fa fa-{{ block.settings.social_icon_3_name }}\"></i>\n                        </a>\n                      {% endif %}\n                    </div>\n                  </div>\n                  <div class=\"team-content text-center\">\n                    {% if section.settings.member_title != '' %}\n                      <h4>{{ block.settings.member_title }}</h4>\n                    {% endif %}\n                    {% if section.settings.post_title != '' %}\n                      <span>{{ block.settings.post_title }}</span>\n                    {% endif %}\n                  </div>\n                </div>\n              </div>\n          {% endcase %}\n        {% endfor %}\n      </div>\n    </div>\n  </div>\n{% endif %}\n\n{%- include 'section-style' -%}",
  "name": "Team Area",
  "settings": [
    {
      "type": "header",
      "content": "Container Style"
    },
    {
      "type": "radio",
      "id": "container_style",
      "label": "Container Style",
      "options": [
        {
          "value": "style_1",
          "label": "Container"
        },
        {
          "value": "style_2",
          "label": "Container Fulied"
        }
      ],
      "default": "style_1"
    },
    {
      "type": "checkbox",
      "id": "team_area_enable",
      "label": "Team Area Enable",
      "default": true
    },
    {
      "type": "header",
      "content": "Section Heading"
    },
    {
      "type": "header",
      "content": "Text Style"
    },
    {
      "type": "radio",
      "id": "section_text_style",
      "label": "Text Style",
      "options": [
        {
          "value": "section_style_left",
          "label": "Left"
        },
        {
          "value": "section_style_center",
          "label": "center"
        },
        {
          "value": "section_style_right",
          "label": "right"
        }
      ],
      "default": "section_style_center"
    },
    {
      "type": "text",
      "id": "section_title",
      "label": "Section Title",
      "default": "Team Members"
    },
    {
      "type": "textarea",
      "id": "section_description",
      "label": "Section Description",
      "default": "Lorem ipsum dolor sit amet conse ctetu."
    }
  ],
  "blocks": [
    {
      "type": "team",
      "name": "team",
      "settings": [
        {
          "type": "text",
          "id": "team_coloum",
          "label": "Team Coloum",
          "default": "3",
          "info": "Follow Bootstrap Column"
        },
        {
          "type": "image_picker",
          "id": "team_image",
          "label": "Image",
          "info": "Recommended Size: 390 x 450px"
        },
        {
          "type": "url",
          "id": "team_image_url",
          "label": "URL"
        },
        {
          "type": "text",
          "id": "member_title",
          "label": "Member Name",
          "default": "Mr.Mike Banding"
        },
        {
          "type": "text",
