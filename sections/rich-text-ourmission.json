{
  "content": "<div class=\"about-mission-area\" id=\"section-our-mission\">\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col-12 gradiant-tittle\" id=\"shopify-section-rich-text\">\n        <div class=\"section-title text-center\">\n          <h2>{{ product.metafields['gardiants-tittle']['gardiants-tittle'] }}</h2>\n        </div>\n      </div>\n      <div class=\"col-lg-3 col-md-6 col-12\">\n        <div class=\"single-mission mb-30\">\n          {{ product.metafields['Key-ingredients-1']['Keyingredients-specs-1'] }}\n        </div>\n      </div>\n      <div class=\"col-lg-3 col-md-6 col-12\">\n        <div class=\"single-mission mb-30\">\n          {{ product.metafields['Key-ingredients-2']['Keyingredients-specs-2'] }}\n        </div>\n      </div>\n      <div class=\"col-lg-3 col-md-6 col-12\">\n        <div class=\"single-mission mb-30\">\n          {{ product.metafields['Key-ingredients-3']['Keyingredients-specs-3'] }}\n        </div>\n      </div>\n\n      <div class=\"col-lg-3 col-md-6 col-12\">\n        <div class=\"single-mission mb-30\">\n          {{ product.metafields['Key-ingredients-4']['Keyingredients-specs-4'] }}\n        </div>\n      </div>\n      <div class=\"col-lg-3 col-md-6 col-12\">\n        <div class=\"single-mission mb-30\">\n          {{ product.metafields['Key-ingredients-5']['Keyingredients-specs-5'] }}\n        </div>\n      </div>\n      <div class=\"col-lg-3 col-md-6 col-12\">\n        <div class=\"single-mission mb-30\">\n          {{ product.metafields['Key-ingredients-6']['Keyingredients-specs-6'] }}\n        </div>\n      </div>\n      <div class=\"col-lg-3 col-md-6 col-12\">\n        <div class=\"single-mission mb-30\">\n          {{ product.metafields['Key-ingredients-7']['Keyingredients-specs-7'] }}\n        </div>\n      </div>\n      <div class=\"col-lg-3 col-md-6 col-12\">\n        <div class=\"single-mission mb-30\">\n          {{ product.metafields['Key-ingredients-8']['Keyingredients-specs-8'] }}\n        </div>\n      </div>\n    </div>\n\n    <div class=\"row\">\n      <div class=\"col-12 btnFIng\">\n        {{ product.metafields['Full-Key-ingredients']['Full-Key-ingredients'] }}\n      </div>\n    </div>\n  </div>\n</div>\n\n<style>\n  .accordionn {\n    color: #444;\n    cursor: pointer;\n    padding: 18px;\n    width: 100%;\n    border: none;\n    text-align: left;\n    outline: none;\n    font-size: 15px;\n    transition: 0.4s;\n  }\n  .panel {\n    padding: 0 18px;\n    display: none;\n    background-color: white;\n    overflow: hidden;\n  }\n</style>\n<script>\n  var acc = document.getElementsByClassName('accordionn');\n  var i;\n\n  for (i = 0; i < acc.length; i++) {\n    acc[i].addEventListener('click', function () {\n      this.classList.toggle('active');\n      var panel = this.nextElementSibling;\n      if (panel.style.display === 'block') {\n        panel.style.display = 'none';\n      } else {\n        panel.style.display = 'block';\n      }\n    });\n  }\n</script>"
