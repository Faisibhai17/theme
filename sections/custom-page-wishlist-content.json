{
  "content": "<style>\n  .empty-list--info p {\n      font-size: clamp(1rem, 0.7368rem + 1.1228vw, 1.875rem);\n      font-weight:500;\n      font-size:30px;\n\n  }\n  .empty-list--info p a {\n      color: #7DCABB !important;\n      text-decoration: underline !important;\n  \tfont-size: clamp(1rem, 0.7368rem + 1.1228vw, 1.875rem);\n      font-size:30px;\n  }\n</style>\n\n<div class=\"customer-page theme-default-margin\">\n  <div class=\"container\">\n    <div class=\"wishlist-page\">\n      {% if customer %}\n        <div class=\"full-width-container wishlist-container\">\n          <!-- Product Tile Grid -->\n          <h2 class=\"wishlist-header\">Wishlist</h2>\n          <div class=\"wishlist-grid row\">\n            {% for product in collections.all.products %}\n              {% include 'wishlist-grid-item' %}\n            {% endfor %}\n            <div class=\"wishlist-grid--empty-list\">\n              <div class=\"empty-list--info\">\n                <h1 class=\"empty-list--text black\">{{ settings.wishlist_empty_title }}</h1>\n                <p>{{ 'cart.general.continue_browsing_html' | t }}</p>\n              </div>\n            </div>\n          </div>\n          <!-- Loader -->\n          <div class=\"wishlist-loader\" style=\"display: none;\">\n            <p class=\"wishlist-loader--text\">Loading...</p>\n          </div>\n        </div>\n      {% else %}\n        <div class=\"login-wishlist\">\n          <h2 class=\"\">{{ settings.login_request_title }}</h2>\n          <a class=\"theme-default-button\" class=\"\" href=\"/account/login\">{{ 'layout.customer.log_in' | t }}</a>\n        </div>\n      {% endif %}\n    </div>\n  </div>\n</div>",
  "name": "Custom page-wishlist Content",
  "settings": []
}