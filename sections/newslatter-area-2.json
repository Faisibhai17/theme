{
  "content": "<div class=\"subscribe-area-3\" id=\"section-{{ section.id }}\">\n  <div class=\"{% if section.settings.container_style == 'style_1' %}container {% else %} container-fluid{% endif %}\">\n    <div class=\"row\">\n      <div class=\"col-xl-5 col-lg-7 ml-auto mr-auto\">\n        <div class=\"subscribe-style-3 text-center\">\n          {% if section.settings.newslatter_title != '' %}\n            <h2>{{ section.settings.newslatter_title }}</h2>\n          {% endif %}\n          {% if section.settings.newslatter_description != '' %}\n            <p>{{ section.settings.newslatter_description }}</p>\n          {% endif %}\n          <div class=\"klaviyo-signup\">\n            <div class=\"klaviyo-form-WTS8es\"></div>\n          </div>\n\n          {% comment %}\n            <div id=\"mc_embed_signup\" class=\"subscribe-form-3 mt-35\">\n              <form action=\"{{ settings.newsletter_action }}\" class=\"newsletter-form mc-form validate\" method=\"post\" id=\"mc-embedded-subscribe-form\"  name=\"mc-embedded-subscribe-form\" target=\"_blank\" novalidate>\n                <div id=\"mc_embed_signup_scroll\" class=\"mc-form\">\n                  <input type=\"email\" value=\"\" name=\"EMAIL\" class=\"newsletter-form__input email\" id=\"mce-EMAIL\" placeholder=\"Enter Your Email Address\" required>\n                  <div class=\"clear-3\">\n                    <input class=\"button\" type=\"submit\" name=\"subscribe\" value=\"{{ section.settings.newslatter_button_title }}\">\n                  </div>\n                </div>\n              </form>\n            </div>\n          {% endcomment %}\n        </div>\n      </div>\n    </div>\n  </div>\n</div>\n\n{%- include 'section-style' -%}",
  "name": "Newslatter Area 2",
  "settings": [
    {
      "type": "header",
