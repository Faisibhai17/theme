{
  "content": "<div class=\"slider-area\" id=\"section-{{ section.id }}\">\n  <div class=\"slider-active-2 owl-carousel nav-style-2\">\n    {% for block in section.blocks %}\n      {% case block.type %}\n        {% when 'slide' %}\n          <div\n            class=\"slider-height-4 d-flex align-items-center bg-img\"\n            style=\"background-image:url({% if block.settings.slider_img %}{{ block.settings.slider_img | img_url: 'master' }}{% else %}http:\\/\\/via.placeholder.com\\/1920x800{% endif %});\"\n          >\n            <div class=\"container\">\n              <div class=\"row\">\n                <div class=\"col-xl-12 col-lg-12 col-md-12 col-12\">\n                  <div class=\"slider-content-5 slider-animated-1 text-{% if block.settings.text_style == 'text_style_left' %}left{% elsif block.settings.text_style == 'text_style_center' %}center{% elsif block.settings.text_style == 'text_style_right' %}right{% endif %}\">\n                    {% if block.settings.slider_subtitle != '' %}\n                      <h3 class=\"animated\">{{ block.settings.slider_subtitle }}</h3>\n                    {% endif %}\n                    {% if block.settings.slider_title != '' %}\n                      <h1 class=\"animated\">{{ block.settings.slider_title }}</h1>\n                    {% endif %}\n                    {% if block.settings.slider_description != '' %}\n                      <p class=\"animated\">{{ block.settings.slider_description }}</p>\n                    {% endif %}\n                    {% if block.settings.button_title != '' %}\n                      <div class=\"slider-btn-5 btn-hover\">\n                        <a class=\"animated\" href=\"{{ block.settings.button_url }}\">{{ block.settings.button_title }}</a>\n                      </div>\n                    {% endif %}\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n      {% endcase %}\n    {% endfor %}\n  </div>\n</div>\n\n{%- include 'section-style' -%}\n\n<script>\n\n\n\n  /* Slider active 2 */\n  $('#section-{{ section.id }} .slider-active-2').owlCarousel({\n      loop: true,\n    {% if settings.rtl_enable %} rtl:true, {% endif %}\n      nav: true,\n      autoplay: false,\n      navText: ['<i class=\"pe-7s-angle-left\"></i>', '<i class=\"pe-7s-angle-right\"></i>'],\n      autoplayTimeout: 5000,\n      animateOut: 'fadeOut',\n      animateIn: 'fadeIn',\n      item: 1,\n      responsive: {\n          0: {\n              items: 1\n          },\n          768: {\n              items: 1\n          },\n          1000: {\n              items: 1\n          }\n      }\n  })\n</script>",
  "name": "Slider 5",
  "settings": [],
  "blocks": [
    {
      "type": "slide",
      "name": "Slide",
      "settings": [
        {
          "type": "image_picker",
          "id": "slider_img",
          "label": "Image",
          "info": "Recommended Size: 1920 x 800px"
        },
        {
          "type": "header",
          "content": "Text Style"
        },
        {
          "type": "radio",
          "id": "text_style",
          "label": "Text Style",
          "options": [
            {
              "value": "text_style_left",
              "label": "Left"
            },
            {
              "value": "text_style_center",
              "label": "center"
            },
            {
              "value": "text_style_right",
              "label": "right"
            }
          ],
          "default": "text_style_center"
        },
        {
          "type": "text",
          "id": "slider_subtitle",
